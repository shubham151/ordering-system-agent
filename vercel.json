{
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api" }
  ],
  "builds": [
    {
      "src": "backend/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/index.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "backend/main.py"
    }
  ]
}